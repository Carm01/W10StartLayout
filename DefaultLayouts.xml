					<start:SecondaryTile AppUserModelID="windows.immersivecontrolpanel_cw5n1h2txyewy!microsoft.windows.immersivecontrolpanel" TileID="SystemSettings_L2_SettingsPageDevicesPrinters" DisplayName="Printers" Size="2x2" Column="4" Row="2" Arguments="page=SettingsPageDevicesPrinters" Square150x150LogoUri="ms-appdata:///local/SettingsTiles/Devices.png" Wide310x150LogoUri="ms-appdata:///local/SettingsTiles/wide.Devices.png" ShowNameOnSquare150x150Logo="true" ShowNameOnWide310x150Logo="true" BackgroundColor="#00000000" ForegroundText="light" />
